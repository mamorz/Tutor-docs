// 5 < 8 || j1++ < 1 && ++j1 < 2;
graph EvaluationOrder {
    rankdir=BT;

    "<3" [label="<"];
    "<2" [label="<"];
    "<1" [label="<"];

    "<1" -- "||";
    "&&" -- "||";

    "5" -- "<1";
    "8" -- "<1";

    "<2" -- "&&";
    "<3" -- "&&";

    "j1++" -- "<2";
    "1" -- "<2";

    "++j1" -- "<3";
    "2" -- "<3";
}
